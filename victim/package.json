    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        },
        "ionic_webpack": "./config/webpack.config.js"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3"
    },
    "cordova": {
