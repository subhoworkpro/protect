{
    "name": "ionic2-taxi-app",
    "version": "0.1.0",
    "description": "Ionic 2 Taxi App - A simple starting point for building 'on-demand/rideshare taxi' ionic application ( something like Uber, Lyft or Sidecar .. )",
    "keywords": [
        "ionic",
        "ionic2",
        "angular2",
        "google-maps",
        "webpack",
        "typescript",
        "yarn"
    ],
    "author": {
        "name": "Damien Dell'Amico",
        "email": "damien.dellamico@gmail.com",
        "url": "https://github.com/ddellamico"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ddellamico/ionic2-taxi-app.git"
    },
    "scripts": {
        "dev": "bnr dev",
        "build": "bnr build",
        "build-dev": "bnr build-dev",
        "clean": "bnr clean",
        "test": "bnr test",
        "test:watch": "bnr test-watch",
        "ios:dev": "bnr ios:dev",
        "ios:release": "bnr ios:release",
        "android:dev": "bnr android:dev",
        "android:release": "bnr android:release",
        "commit": "git-cz",
        "release": "standard-version --no-verify",
        "push": "git push --follow-tags origin master",
        "lint": "tslint \"src/**/*.ts\" --exclude=src/**/*.d.ts",
        "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
        "outdated": "yarn outdated",
        "precommit": "npm run lint",
        "prepush": "npm test"
    },
    "betterScripts": {
        "dev": {
            "command": "ionic-app-scripts serve",
            "env": {
                "NODE_ENV": "development",
                "PLATFORM": "web"
            }
        },
        "build": {
            "command": "ionic-app-scripts build --prod",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "build-dev": {
            "command": "ionic-app-scripts build --dev",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "clean": {
            "command": "ionic-app-scripts clean"
        },
        "test": {
            "command": "karma start",
            "env": {
                "NODE_ENV": "test"
            }
        },
        "test-watch": {
            "command": "karma start --auto-watch --no-single-run",
            "env": {
                "NODE_ENV": "test",
                "NO_COVERAGE": true
            }
        },
        "ios:dev": {
            "command": "ionic run ios --livereload",
            "env": {
                "NODE_ENV": "development",
                "PLATFORM": "ios"
            }
        },
        "ios:release": {
            "command": "ionic build ios --prod",
            "env": {
                "NODE_ENV": "production",
                "PLATFORM": "ios"
            }
        },
        "android:dev": {
            "command": "ionic run android --livereload",
            "env": {
                "NODE_ENV": "development",
                "PLATFORM": "android"
            }
        },
        "android:release": {
            "command": "ionic build android --prod",
            "env": {
                "NODE_ENV": "production",
                "PLATFORM": "android"
            }
        }
    },
    "dependencies": {
        "@angular/common": "2.4.8",
        "@angular/compiler": "2.4.8",
        "@angular/compiler-cli": "2.4.8",
        "@angular/core": "2.4.8",
        "@angular/forms": "2.4.8",
        "@angular/http": "2.4.8",
        "@angular/platform-browser": "2.4.8",
        "@angular/platform-browser-dynamic": "2.4.8",
        "@angular/platform-server": "2.4.8",
        "@angular/tsc-wrapped": "~0.5.2",
        "@ionic-native/camera-preview": "^3.10.3",
        "@ionic-native/core": "^3.10.3",
        "@ionic-native/local-notifications": "^3.12.1",
        "@ionic-native/media-capture": "^3.12.1",
        "@ionic-native/native-audio": "^3.12.1",
        "@ionic-native/photo-viewer": "^4.1.0",
        "@ionic-native/shake": "^3.11.0",
        "@ionic-native/vibration": "^3.11.0",
        "@ionic/storage": "2.0.0",
        "angularfire2": "^2.0.0-beta.8",
        "chart.js": "^2.6.0",
        "com-sarriaroman-photoviewer": "^1.1.10",
        "cordova-android": "^6.2.3",
        "cordova-plugin-app-event": "file:node_modules\\cordova-plugin-app-event",
        "cordova-plugin-camera-preview": "^0.9.0",
        "cordova-plugin-console": "~1.0.4",
        "cordova-plugin-device": "~1.1.3",
        "cordova-plugin-device-motion": "^1.2.5",
